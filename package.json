{
  "name": "webpack-library-starter",
  "version": "0.0.0",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/webpack-library-starter.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest",
    "coverage": "npm run test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pruge/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "james kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pruge/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/pruge/webpack-library-starter",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jest": "^24.8.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "nyc": "^13.1.0",
    "open-cli": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "webpack-source-map-support": "^2.0.1",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
